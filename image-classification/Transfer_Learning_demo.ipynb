   "source": [
    "from sklearn.preprocessing import LabelBinarizer\n",
    "\n",
    "lb = LabelBinarizer()\n",
    "lb.fit(labels)\n",
    "\n",
    "labels_vecs = lb.transform(labels)\n",
    "print(labels_vecs[0])\n",
    "print(labels_vecs[1000])\n",
    "print(labels_vecs[2000]) "
